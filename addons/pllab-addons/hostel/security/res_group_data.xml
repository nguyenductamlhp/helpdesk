<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <!-- ======== [Feature Groups] ======== -->
        <record id="group_hostel_viewer" model="res.groups">
            <field name="name">Hostel Viewer</field>
        </record>
        <record id="group_hostel_user" model="res.groups">
            <field name="name">Hostel User</field>
            <field name="implied_ids"
                eval="[(4, ref('hostel.group_hostel_viewer'))]"/>
        </record>
        <record id="group_hostel_manager" model="res.groups">
            <field name="name">Hostel Admin</field>
            <field name="implied_ids"
                eval="[(4, ref('hostel.group_hostel_user'))]"/>
        </record>
    </data>
</odoo>