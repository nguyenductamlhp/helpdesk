<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <menuitem
        id="hostel_root_menu"
        name="Hostel"
        sequence="25"
        groups="base.group_user" />

    <record id="action_hostel_room" model="ir.actions.act_window">
        <field name="name">Room</field>
        <field name="res_model">hostel.room</field>
        <field name="view_mode">tree,form</field>
    </record>
    <menuitem id="menu_hostel_room_view"
        name="Room"
        sequence="10"
        action="action_hostel_room"
        parent="hostel_root_menu"
        groups="base.group_user"/>

    <record id="action_hostel_invoice" model="ir.actions.act_window">
        <field name="name">Invoice</field>
        <field name="res_model">hostel.invoice</field>
        <field name="view_mode">tree,form</field>
    </record>
    <menuitem id="menu_hostel_invoice_view"
        name="Invoice"
        sequence="20"
        action="action_hostel_invoice"
        parent="hostel_root_menu"
        groups="base.group_user"/>

</odoo>
